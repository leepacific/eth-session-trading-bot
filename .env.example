# Railway 환경 변수 설정 예시

# Trading Bot API URL (Railway에서 자동 생성됨)
TRADING_BOT_URL=https://your-app.railway.app

# Discord 웹훅 URL (선택사항 - 알림용)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your-webhook-url

# Railway API 설정 (선택사항)
RAILWAY_API_URL=https://api.railway.app

# 포트 설정 (Railway에서 자동 설정됨)
PORT=8000

# 로그 레벨
LOG_LEVEL=INFO

# 거래 설정
INITIAL_BALANCE=10000
MAX_DAILY_TRADES=10
RISK_PER_TRADE=0.02