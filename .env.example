# Binance API Configuration
BINANCE_API_KEY=your_api_key_here
BINANCE_SECRET_KEY=your_secret_key_here

# Optional: Testnet (set to true for testing)
BINANCE_TESTNET=false

# Data Collection Settings
DATA_SYMBOL=ETHUSDT
DATA_INTERVAL=15m
DATA_POINTS_TARGET=500000
DATA_POINTS_PER_REQUEST=1000

# Trading Settings
INITIAL_BALANCE=100000
MAX_ACCOUNT_RISK_PER_TRADE=0.05
LIQUIDATION_PROBABILITY=0.07
MAX_LEVERAGE=125

# Railway Settings
RAILWAY_PUBLIC_DOMAIN=your-app.railway.app
RAILWAY_STATIC_IP=your_static_ip

# Auto Optimization Settings
ENABLE_AUTO_OPTIMIZATION=true
OPTIMIZATION_SCHEDULE=weekly
RESOURCE_LIMIT_CPU=0.7
RESOURCE_LIMIT_MEMORY=0.7
OPTIMIZATION_TIMEZONE=Asia/Seoul